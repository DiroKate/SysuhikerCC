{
  "disableCSSModules": true,
  "entry": "src/index.js",
  "theme": {
    "@primary-color": "#1DA57A",
    "@link-color": "#1DA57A"
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        [
          "import",
          {
            "libraryName": "antd",
            "style": true
          }
        ]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    }
  }
}
